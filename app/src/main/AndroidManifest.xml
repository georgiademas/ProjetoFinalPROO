<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.smartlifeti.estagioweb">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".control.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".control.InscreverSeActivity" />
        <activity android:name=".control.MainEmpresaActivity" />
        <activity android:name=".control.CadastroEstagioFormActivity" />
        <activity android:name=".control.PrimeiroAcessoActivity" />
        <activity android:name=".control.InscreverEstudanteActivity" />
        <activity android:name=".control.InscreverEmpresaActivity" />
        <activity android:name=".control.MainEstudanteActivity" />
        <activity android:name=".control.MainCoordenadorActivity" />
        <activity android:name=".control.MainVisitanteActivity"></activity>
    </application>

</manifest>
